{
  "ConnectionStrings": {
    "Primary": "Data Source=db/mira.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System.Net.Http.HttpClient": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Discord": {
    "Token": "place-discord-token-here"
  },
  "PollingOptions": {
    "NewHostIntervalSeconds": 45,
    "NewHostValidationTimeoutSeconds": 1
  }, 
  "FallbackAuthHeaders": {
    "www_urltotarget_com": "auth-header"
  }
}